<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/5.4/option.xsd">
	<import>
		<categories>
			<category name="minecraft_sync">
				<parent>minecraft</parent>
			</category>
			<category name="minecraft_sync_general">
				<parent>minecraft_sync</parent>
			</category>
		</categories>
		<options>
			<option name="minecraft_sync_enabled">
				<categoryname>minecraft_sync_general</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
				<enableoptions>minecraft_sync_identity,minecraft_sync_entries_per_call</enableoptions>
			</option>
			<option name="minecraft_sync_identity">
				<categoryname>minecraft_sync_general</categoryname>
				<optiontype>MinecraftConnectionMultiSelect</optiontype>
			</option>
			<option name="minecraft_sync_entries_per_call">
				<categoryname>minecraft_sync_general</categoryname>
				<optiontype>integer</optiontype>
				<defaultvalue>100</defaultvalue>
			</option>
		</options>
	</import>
</data>
