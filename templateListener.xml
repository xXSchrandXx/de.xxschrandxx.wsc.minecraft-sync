	<import>
		<!-- admin -->
		<templatelistener name="groupAddSection">
			<environment>admin</environment>
			<templatename>userGroupAdd</templatename>
			<eventname>sections</eventname>
			<templatecode><![CDATA[{include file='__minecraftSyncGroupAddSection'}]]></templatecode>
		</templatelistener>
		<templatelistener name="userAddButton">
			<environment>admin</environment>
			<templatename>userAdd</templatename>
			<eventname>addMinecraftButtons</eventname>
			<templatecode><![CDATA[{include file='__minecraftSyncUserAddButtons'}]]></templatecode>
		</templatelistener>
		<!-- /admin -->
	</import>
</data>
